/contracts/utils/introspection/IERC165.sol\":{\"keccak256\":\"0x447a5f3ddc18419d41ff92b3773fb86471b1db25773e07f877f548918a185bf1\",\"license\":\"MIT\",\"urls\":[\"bzz-raw://be161e54f24e5c6fae81a12db1a8ae87bc5ae1b0ddc805d82a1440a68455088f\",\"dweb:/ipfs/QmP7C3CHdY9urF4dEMb9wmsp1wMxHF6nhA2yQE5SKiPAdy\"]},\"project:/contracts/ERC2981Base.sol\":{\"keccak256\":\"0x3e772728c07ffbed5e4e215a7a7e4ea4f8650d4e5979bdf1ef9baf39c209556c\",\"license\":\"MIT\",\"urls\":[\"bzz-raw://10dbdf109ded31a4e9edd230c05279aa0f7df2640f972a9b7f63bf63c72d0498\",\"dweb:/ipfs/QmP9dNrnwjnBYiue5nBiziTsPcF99mfrP6soeE4zRDs826\"]},\"project:/contracts/ERC2981PerTokenRoyalties.sol\":{\"keccak256\":\"0x7f29b0baedfb00970ba532e479441314b3419efcf9dcaae381af551eb5493274\",\"license\":\"MIT\",\"urls\":[\"bzz-raw://68730ce3e363c55bbff29d271526664618489467f713b1b6fe54dd0b2f6ed249\",\"dweb:/ipfs/QmSNPnwTn57mZSuD7qRjkm66bFVjMCkTuLfwwZ9pPUKLHp\"]},\"project:/contracts/IERC2981Royalties.sol\":{\"keccak256\":\"0x03505eca18250067cbb318f2a51f8e26d0810ace0248c9e791d9b689913ffb14\",\"license\":\"MIT\",\"urls\":[\"bzz-raw://88b316d304f99419148a18b2c08cac6858ee50fd76f9b3914e5c0d3c6266ef21\",\"dweb:/ipfs/QmdsiCnSYnxb6BHDG9fcJYbuG28kWxrBFWW9Ltzi76cxCS\"]}},\"version\":1}",
